[application]

config/name="TreasureHunters"
run/main_scene="res://Maps/Sandbox.tscn"
config/features=PackedStringArray("4.0", "Mobile")
config/icon="res://icon.svg"

[autoload]

EventBus="*res://Singletons/EventBus.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
InputHelper="*res://addons/input_helper/input_helper.gd"
SoundManager="*res://addons/sound_manager/sound_manager.tscn"

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_height"

[editor]

scene/scene_naming=1
version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg", "res://addons/input_helper/plugin.cfg", "res://addons/sound_manager/plugin.cfg")

